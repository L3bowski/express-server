	                        <div class="row">
	                            <div class="col-md-3 col-sm-4 product-window" ng-repeat="product in container.products">
	                                <img ng-src="{{product.url}}" width="100%" />
                                    <h4>{{product.name}}</h4>
	                            </div>
	                        </div>
	                    </div>
                    </div>
