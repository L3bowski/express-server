    <section id="contact" class="palette-bg-4">

        <div class="vertical-aligner">
            <div class="vertical-aligned">
                <div class="container spaced">
                    <div class="row white-text">
                        <div class="col-xs-12">
                            <h2 style="margin-bottom: 20px;">{{content.current.Title}}</h2>
                        </div>
                        <div class="col-xs-12">                        
                            <h4>{{content.current.Phone}}: +34 93 852 91 41</h4>
                            <h4>{{content.current.Mail}}: info@carniquesausa.com</h4>
                            <h4 style="margin-bottom: 20px;">{{content.current.Address}}: C-17 km.83, 08585, Montesquiu</h4>

                        </div>
                    </div>
                </div>
