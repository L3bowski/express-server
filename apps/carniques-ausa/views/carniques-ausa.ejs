<html xmlns="http://www.w3.org/1999/xhtml" lang="ca" xml:lang="ca">
<head>

    <title>Càrniques Ausa</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="shortcut icon" type="image/png" href="images/favicon.png" />

    <link rel="stylesheet" href="/plugins/bootstrap-3.2.0/css/bootstrap.min.css" type="text/css" />
    <link rel="stylesheet" href="/plugins/font-awesome-4.4.0/css/font-awesome.css" type="text/css" />
    <link rel="stylesheet" href="/plugins/responsive-slides-1.55/css/responsiveslides.css" type="text/css">
    <link rel="stylesheet" href="/plugins/responsive-slides-1.55/css/custom.css" type="text/css">

    <link rel="stylesheet" type="text/css" href="<%= asset('css/navigation-layout.css') %>" />
    <link rel="stylesheet" type="text/css" href="<%= asset('css/navigation-clickables.css') %>" />
    <link rel="stylesheet" type="text/css" href="<%= asset('css/navigation-branding.css') %>" />
    <link rel="stylesheet" type="text/css" href="<%= asset('css/carniques-ausa.css') %>" />
    <link rel="stylesheet" type="text/css" href="<%= asset('css/commons.css') %>" />
    <link rel="stylesheet" type="text/css" href="<%= asset('css/palette-red.css') %>" />

    <!-- FONT -->
    <link href='https://fonts.googleapis.com/css?family=Raleway:400,500' rel='stylesheet' type='text/css'>

</head>

<body ng-app="Content" ng-controller="ContentCtrl" class="carles-capellas carniques-ausa">

    <div class="navigation-bar palette-bg-2">

        <div class="navigation-small">
            <img id="bars-button" src="<%= asset('images/logo-carniques.png') %>" style="width: 45px; height:45px;" />
            <div id="current-tab"></div>
        </div>

        <div class="navigation-left">
            <span class="navigation-clickable hidden-xs" href="javascript:;">
                <img src="<%= asset('images/logo-carniques.png') %>" style="width: 45px; height:45px;" />
            </span>
            <a class="navigation-clickable" href="#home">
                <i class="fa fa-home"></i>&nbsp;&nbsp;{{content.current.Home}}
            </a>
            <a class="navigation-clickable" href="#values">
                <i class="fa fa-plus-square-o"></i>&nbsp;&nbsp;{{content.current.Values}}
            </a>
            <a class="navigation-clickable" href="#about-us">
                <i class="fa fa-info"></i>&nbsp;&nbsp;{{content.current.AboutUs}}
            </a>
            <a class="navigation-clickable" href="#catalog">
                <i class="fa fa-list-ul"></i>&nbsp;&nbsp;{{content.current.Catalog}}
            </a>
            <a class="navigation-clickable" href="#contact">
                <i class="fa fa-envelope-o"></i>&nbsp;&nbsp;{{content.current.Contact}}
            </a>
        </div>

        <div class="navigation-right">
            <div class="dropdown-wrapper">
                <a class="navigation-clickable dropdown-button">
                    <i class="fa fa-globe"></i>&nbsp;&nbsp;{{content.current.Language}}
                    <span class="dropdown-arrow"></span>
                </a>
                <div class="dropdown-list">
                    <a class="dropdown-clickable" ng-click="content.translate('es')">
                        <img ng-class="content.currentLanguage != 'es' ? 'transparent' : ''"
                            src="<%= asset('images/navbar/spanish-squared.png') %>" height="15" />
                        {{content.current.Spanish}}
                    </a>
                    <a class="dropdown-clickable" ng-click="content.translate('en')">
                        <img ng-class="content.currentLanguage != 'en' ? 'transparent' : ''"
                            src="<%= asset('images/navbar/english-squared.png') %>" height="15" />
                        {{content.current.English}}
                    </a>
                </div>
            </div>
        </div>
    </div>
    <section id="home" class="responsive-bg home-bg">
        <div class="vertical-aligner">
            <div class="vertical-aligned">
                <div class="container spaced">
                </div>
            </div>
        </div>
    </section>

    <section id="values" class="responsive-bg palette-bg-3">
        <div class="vertical-aligner" style="height: auto; padding-bottom: 50px; padding-top: 30px;">
            <div class="vertical-aligned">
                <div class="container spaced">
                    <div class="row white-text">
                        <div class="col-sm-4">
                            <center><i class="fa fa-pencil-square-o fa-5x"></i></center>
                            <p style="margin-top: 30px" ng-bind-html="content.current.Quality | html"></p>
                        </div>
                        <div class="col-sm-4">
                            <center><i class="fa fa-globe fa-5x"></i></center>
                            <p style="margin-top: 30px">{{content.current.Globalization}}</p>
                        </div>
                        <div class="col-sm-4">
                            <center><i class="fa fa-user-plus fa-5x"></i></center>
                            <p style="margin-top: 30px">{{content.current.Confidence}}</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <div class="callbacks_container">
      <ul class="rslides" id="responsive-slider">
        <li>
          <img src="<%= asset('images/slider/1.jpg') %>" alt="">
        </li>
        <li>
          <img src="<%= asset('images/slider/2.jpg') %>" alt="">
        </li>
        <li>
          <img src="<%= asset('images/slider/3.jpg') %>" alt="">
        </li>
        <li>
          <img src="<%= asset('images/slider/4.jpg') %>" alt="">
        </li>
        <li>
          <img src="<%= asset('images/slider/5.jpg') %>" alt="">
        </li>
        <li>
          <img src="<%= asset('images/slider/6.jpg') %>" alt="">
        </li>
      </ul>
    </div>

    <section id="about-us" class="responsive-bg palette-bg-4">
        <div class="vertical-aligner" style="height: auto; padding-bottom: 50px; padding-top: 30px;">
            <div class="vertical-aligned">
                <div class="container spaced">
                    <div class="row white-text">
                        <div class="col-xs-12">
                            <h1>{{content.current.AboutUsTitle}}</h1>
                            <div ng-bind-html="content.current.AboutUsBody | html"></div>
                            <h1>{{content.current.WhatWeDoTitle}}</h1>
                            <div ng-bind-html="content.current.WhatWeDoBody | html"></div>
                        </div>
                    </div>
                    <div class="row text-center">
                        <div class="col-sm-6" style="margin-top:50px;">
                            <a onclick="window.open('<%= asset('pdfs/IFS.pdf') %>');" href="javascript:;" style="color: white; text-decoration: none; font-size: 30px;">
                                <i class="fa fa-file-pdf-o"></i>
                                &nbsp;&nbsp;Certificado IFS
                            </a>
                        </div>
                        <div class="col-sm-6" style="margin-top:50px;">
                            <a onclick="window.open('<%= asset('pdfs/993.pdf') %>');" href="javascript:;" style="color: white; text-decoration: none; font-size: 30px;">
                                <i class="fa fa-file-pdf-o"></i>
                                &nbsp;&nbsp;Certificado 993
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

